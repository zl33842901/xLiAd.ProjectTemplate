                QueryBusinessTypeConfirm: function () {
                    var r = this.$refs.bttree.getCheckedNodes();
                    var rst = [];
                    for (var i = 0; i < r.length; i++) {
                        if (r[i].Children.length < 1) {
                            rst.push(r[i]);
                        }
                    }
                    this.QueryBusinessTypeSelectedNodes = rst;
                    this.showSelectBusinessType = false;
                },
                QueryBusinessTypeClose: function (tag) {
                     this.QueryBusinessTypeSelectedNodes.splice(this.QueryBusinessTypeSelectedNodes.indexOf(tag), 1);
                },
